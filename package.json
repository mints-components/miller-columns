{
  "name": "@mints/miller-columns",
  "version": "2.0.0",
  "keywords": [
    "miller-columns",
    "mints-components",
    "mui"
  ],
  "description": "Miller Columns for Mints Components",
  "license": "MIT",
  "packageManager": "yarn@3.6.1",
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "types": "types",
  "main": "./dist/miller-columns.umd.cjs",
  "module": "./dist/miller-columns.js",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/miller-columns.js"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./dist/miller-columns.umd.cjs"
      }
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf types",
    "build": "yarn clean && tsc -p tsconfig.build.json && vite build",
    "lint": "eslint . --fix",
    "prettier": "prettier . --write",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublish": "yarn build",
    "publish": "yarn npm publish --access public",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fontsource/roboto": "^5.0.14",
    "@mui/material": "^5.16.7",
    "@mui/styled-engine-sc": "^6.0.0-alpha.18",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "styled-components": "^6.1.12"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@^6.0.0-alpha.18"
  },
  "peerDependencies": {
    "@fontsource/roboto": "^5.0.14",
    "@mui/material": "^5.16.7",
    "@mui/styled-engine-sc": "^6.0.0-alpha.18",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "styled-components": "^6.1.12"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.8.0",
    "@mints/eslint-config": "^1.0.2",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@types/node": "^20.4.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "pinst": "^3.0.0",
    "prettier": "^3.3.2",
    "storybook": "^8.2.9",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
